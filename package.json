{
  "name": "450-dsa-tracker-fullstack",
  "version": "1.0.0",
  "main": "index.ts",
  "repository": "https://github.com/sounishnath003/450-DSA-Tracker.git",
  "author": "Sounish Nath <flock.sinasini@gmail.com>",
  "license": "MIT",
  "scripts": {
    "module:install": "concurrently -m=5 \"yarn server:install\" \"&&\" \"yarn client:install\"",
    "dev": "concurrently -m=3 \"yarn start:server\" \"yarn start:client\"",
    "start:server": "cd server && yarn dev",
    "start:client": "cd client && yarn start",
    "dev:install": "concurrently -m=5 \"yarn server:install\" \"yarn client:install\"",
    "server:install": "cd server && yarn install",
    "client:install": "cd client && yarn install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.0.0"
  }
}
